{
    "family": "onemind-backend-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "onemind",
        "image": " 072365266667.dkr.ecr.ap-south-1.amazonaws.com/onemind",
        "portMappings": [
          {
            "containerPort": 5000,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ]
  }
  